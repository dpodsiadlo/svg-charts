{
  "name": "dpodsiadlo/svg-charts",
  "description": "SVG Charts Generator for Laravel & DOMPDF",
  "keywords": [
    "laravel",
    "svg",
    "charts"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "illuminate/view": ">=5.2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "DPodsiadlo\\SvgCharts\\Providers\\SvgChartsProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "DPodsiadlo\\SvgCharts\\": "/src/"
    }
  }
}
